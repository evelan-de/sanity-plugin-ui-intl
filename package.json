{
  "name": "sanity-plugin-ui-intl",
  "version": "1.0.0",
  "description": "Sanity plugin for the multilingual Studio user interface\r",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/evelan-de/sanity-plugin-ui-intl#readme",
  "bugs": {
    "url": "https://github.com/evelan-de/sanity-plugin-ui-intl/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/evelan-de/sanity-plugin-ui-intl.git"
  },
  "license": "MIT",
  "author": "Evelan GmbH <kontakt@evelan.de>",
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict",
    "prepare": "husky"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "1.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@sanity/pkg-utils": "7.11.5",
    "@sanity/plugin-kit": "4.0.19",
    "@sanity/semantic-release-preset": "5.0.0",
    "@types/react": "18.3.3",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-sanity": "7.1.4",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.19",
    "sanity": "4.3.0",
    "styled-components": "6.1.19",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "sanity": "^3 || ^4"
  },
  "engines": {
    "node": ">=22"
  }
}
